{
  "expo": {
    "name": "TimesOfTech",
    "slug": "timesoftech",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/YHN-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff6600"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/YHN-logo-fg.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "package": "com.akshat312002.timesoftech"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-audio",
      "expo-splash-screen"
    ],
    "scheme": "timesoftech",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "75ea6387-e903-4360-9487-0583f483651c"
      }
    }
  }
}
